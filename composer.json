{
  "name": "gabrielbull/ups-api",
  "type": "library",
  "description": "PHP UPS API",
  "keywords": [
    "ups",
    "api",
    "wrapper",
    "quantum view",
    "tracking",
    "shipping",
    "rate",
    "rating",
    "time in transit",
    "tradeability"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    },
    {
      "name": "Sebastien Vergnes",
      "email": "sebastien@vergnes.eu"
    },
    {
      "name": "Michael Williams",
      "email": "michael.williams@limelyte.com"
    },
    {
      "name": "Stefan Doorn",
      "email": "stefan@efectos.nl"
    },
    {
      "name": "Eduard Sukharev",
      "email": "sukharev.eh@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "psr/log": "^1.0 || ^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.4"
  },
  "suggest": {
    "ext-soap": "Required if you need the Tradeability API of UPS"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Ups\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ups\\Tests\\": [
        "tests/Ups/Tests",
        "tests/Ups/Tests/_includes"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
