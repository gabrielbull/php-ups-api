{
  "name": "gabrielbull/ups-api",
  "type": "library",
  "description": "PHP UPS API",
  "keywords": ["ups", "api", "wrapper", "quantum view", "tracking", "shipping", "rate", "rating", "time in transit"],
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    },
    {
      "name": "Sebastien Vergnes",
      "email": "sebastien@vergnes.eu"
    },
    {
      "name": "Michael Williams",
      "email": "michael.williams@limelyte.com"
    },
    {
      "name": "Stefan Doorn",
      "email": "stefan@efectos.nl"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "~6.0@dev|>=6",
    "psr/log": "~1.0|1.0.0|~1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "autoload": {
    "psr-4": {
      "Ups\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ups\\Tests\\": ["tests/Ups/Tests", "tests/Ups/Tests/_includes"]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.5.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
